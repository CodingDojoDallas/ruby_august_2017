<h1>Listing <%= @dojos.length %> Dojos</h1>

<% if flash[:success]%>
<div class="success"><%= flash[:success] %></div>
<% end %>
<br>
<br>

<table>
    <tbody>
        <th>Coding Dojo Branch</th>
        <th>Street</th>
        <th>City</th>
        <th>State</th>
    </tbody>
    <% @dojos.each do |dojo| %>
    <tr>
        <td><%= dojo.branch%></td>
        <td><%= dojo.street%></td>
        <td><%= dojo.city%></td>
        <td><%= dojo.state%></td>
        <td><a href="/show/<%= dojo.id %>">Show</a></td>
        <td><a href="/edit/<%= dojo.id %>">Edit</a></td>
        <td><a href="/destroy/<%= dojo.id %>">Destroy</a></td>
    </tr>
    <% end %>
</table>

<a href='/dojos/new'>New Dojo</a>
