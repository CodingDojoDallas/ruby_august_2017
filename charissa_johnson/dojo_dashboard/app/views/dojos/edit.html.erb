<h1>Editing <%= @dojo.branch %></h1>
<form action="/dojos/<%= @dojo.id %>" method="post">
	<input type="hidden" name="_method" value="patch"/>
	<input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>"/>
	<input type="text" name="dojo_info[branch]" value="<%= @dojo.branch %>"/>
	<input type="text" name="dojo_info[street]" value="<%= @dojo.street %>"/>
	<input type="text" name="dojo_info[city]" value="<%= @dojo.city %>"/>
	<input type="text" name="dojo_info[state]" value="<%= @dojo.state %>"/>
	<input type="submit" value="Update"/>
</form>

<a href="/dojos">Home</a>
<a href="/dojos/<%= @dojo.id %>">Show</a>