<h1>Listing 3 Dojos</h1>
<table>
  <tr>
    <th>Coding Dojo Branch</th>
    <th>Street</th>
    <th>City</th>
    <th>State</th>
  </tr>
  <% @dojos.each do |d| %>
    <tr>
      <td><%= d['name'] %></td>
      <td><%= d['street'] %></td>
      <td><%= d['city'] %></td>
      <td><%= d['state'] %></td>
    </tr>
  <% end %>
</table>

<div>
  <a href="/dojos/new">Create new dojo</a>
</div>
